{% extends 'base.html.twig' %}

{% block title %}Affiliate{% endblock %}

{% block body %}
    <h1>Affiliate</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ affiliate.id }}</td>
        </tr>
        <tr>
            <th>Url</th>
            <td>{{ affiliate.url }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ affiliate.email }}</td>
        </tr>
        <tr>
            <th>Token</th>
            <td>{{ affiliate.token }}</td>
        </tr>
        <tr>
            <th>Active</th>
            <td>{{ affiliate.active ? 'Yes' : 'No' }}</td>
        </tr>
        <tr>
            <th>CreatedAt</th>
            <td>{{ affiliate.createdAt ? affiliate.createdAt|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('affiliate_list') }}">back to list</a>

    <a href="#">edit</a>

{% endblock %}
