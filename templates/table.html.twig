<table class="table text-center">
    <thead>
    <tr>
        <th class="active text-center">City</th>
        <th class="active text-center">Position</th>
        <th class="active text-center">Company</th>
        <th class="active text-center">Created at</th>
    </tr>
    </thead>

    <tbody>
    {% for job in jobs %}
    <tr>
        <td> {{ job.location }}</td>
        <td>
            <a href="{{ url('job.show', {id: job.id}) }}">
                {{ job.position }}
            </a>
        </td>
        <td>{{ job.company }}</td>
        <td>{{ job.createdAt|date("d/m/Y") }}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>
